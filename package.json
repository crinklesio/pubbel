{
  "name": "pubbel",
  "version": "4.0.0",
  "description": "lightweight dependency free event-driven JavaScript components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib dist es tsconfig.tsbuildinfo",
    "test:watch": "npm test -- --watchAll --coverage",
    "build": "npm run clean && tsc && tsc -b ./tsconfig.json && npm run bundlesize",
    "prepublish": "npm run build",
    "bundlesize": "bundlesize --config bundlesize.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevtiq/pubbel.git"
  },
  "keywords": [
    "pub sub",
    "event-driven",
    "event",
    "storage",
    "queue"
  ],
  "author": "Kevin Pennekamp <github@kevtiq.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevtiq/pubbel/issues"
  },
  "homepage": "https://github.com/kevtiq/pubbel#readme",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "bundlesize": "^0.18.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^26.0.1",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {}
}
